<h1>Listing Cloud Accounts</h1>


<table border=+1>
  <tr>
    <th>Provider</th>
    <th>Username</th>
    <th>Apikey</th>
    <th>Privatekey</th>
    <th>Certificate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cloud_accounts.each do |cloud_account| %>
  <tr>
    <td><%= cloud_account.provider %></td>
    <td><%= cloud_account.username %></td>
    <td><%= cloud_account.apikey %></td>
    <td><%= cloud_account.privatekey %></td>
    <td><%= cloud_account.certificate %></td>
    <td><%= link_to 'Show', cloud_account %></td>
    <td><%= link_to 'Edit', edit_cloud_account_path(cloud_account) %></td>
    <td><%= link_to 'Destroy', cloud_account, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cloud account', new_cloud_account_path %>
